%div#log_in

  -if flash[:error] 
    %p.center= flash[:error]
  - if flash[:alert]
    %p.center= flash[:alert]

  %h2 Log in

  = form_tag sessions_path do
    %p
      = label_tag :email 
      %br
      = text_field_tag :email, params[:email]
    %p
      = label_tag :password 
      %br
      = password_field_tag :password 
    %p
      = submit_tag "Log in" 

