%table{:id => "exp_mzid_files_table", :class =>"my_table inner" }
  %thead
    %tr
      %th.mzid_file_name .mzid File
      %th.submission_date submission date
      %th{colspan: 2}
  %tbody  
    -mzid_files.each do |mzidf|
      %tr
        %td= mzidf.name
        %td= mzidf.submission_date
        %td.center
          = link_to '<Spectrum Identification>', mzid_file_spectrum_identifications_path(mzidf.id), id:"mzidf_#{mzidf.id}_si", class: "flat_link"
        %td.center
          - if mzidf.includes_protein_detection
            =link_to '<Protein Detection>', mzid_file_protein_detections_path(mzidf.id), id: "mzid_file_#{mzidf.id}_protein_detection", class: "flat_link"
          - else
            No Protein Detection 
