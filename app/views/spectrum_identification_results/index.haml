%h3= @protocol
%h4= "Spectrum Identification: #{@si.si_id} - Spectrum Identification List:  #{@sil.sil_id}"
%p
  %h4= "Spectrum Identification Results"
%table#sir_table.my_table{:data => {'si-id' => "#{@si.id}", 'source' => spectrum_identification_results_url(format: "json")}}
  %thead
    %tr
      %th Spectrum Result ID
      %th Spectrum Name
      %th Spectrum ID
  %tbody
    -#= render @spectrum_identification_results
%br
%p
  %h4= "Spectrum Identification Items"
%table#sii_table.my_table
  %thead
    %tr
      %th SII ID
      %th calc m/z
      %th exp m/z
      %th rank
      %th charge
      %th pass thrshld
  %tbody


