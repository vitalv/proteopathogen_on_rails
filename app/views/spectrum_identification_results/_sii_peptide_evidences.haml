- if  @peptide_evidences.blank?
  %tr
    %td.empty{colspan: 6}= "No peptide evidence(s) found"
- else
  - @peptide_evidences.each do |pep_ev|
    %tr
      %td.center= pep_ev.start
      %td.center= pep_ev.end
      %td.center= pep_ev.pre
      %td.center= pep_ev.post
      %td.center= pep_ev.is_decoy || pep_ev.matches_decoy_protein
      %td.center= pep_ev.db_sequence.accession
